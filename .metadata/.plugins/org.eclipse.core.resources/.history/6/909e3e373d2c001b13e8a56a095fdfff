package ex1.test;

import java.util.List;

import ex1.WGraph_DS;
import ex1.src.WGraph_Algo;
import ex1.src.node_info;

public class Tester {

	public static void main(String[] args) {
		WGraph_DS g = new WGraph_DS();
		g.addNode(1);
		g.addNode(2);
		g.addNode(3);
		g.addNode(4);
		g.addNode(5);
		g.connect(1, 2, 6);
		g.connect(1, 4, 1);
		g.connect(2, 4, 2);
		g.connect(2, 5, 2);
		g.connect(2, 3, 5);
		g.connect(3, 5, 5);
		g.connect(4, 5, 1);
		WGraph_Algo wg= new WGraph_Algo(g);
		List<node_info> l = wg.shortestPath(1, 2);
		g.connect(4, 5, 1);
	}

}
