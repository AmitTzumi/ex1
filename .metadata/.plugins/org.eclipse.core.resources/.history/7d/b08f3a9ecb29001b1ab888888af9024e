package ex1;

public class Tester {

	public static void main(String[] args) {
		WGraph_DS g = new WGraph_DS();
		g.addNode(1);
		g.addNode(2);
		g.addNode(3);
		g.addNode(4);
		g.addNode(5);
		g.connect(1, 2, 6);
		g.connect(1, 4, 1);
		g.connect(2, 4, 2);
		g.connect(2, 5, 2);
		g.connect(2, 3, 5);
		g.connect(3, 5, 5);
		g.connect(4, 5, 1);
		WGraph_Algo wg= new WGraph_Algo(g);
		 System.out.println(wg.shortestPathDist(1, 4));
	}

}
